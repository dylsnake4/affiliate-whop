    display: 'flex',
    flexDirection: 'column',
    transition: 'transform 0.3s ease',
    zIndex: 1000,
  },
  logo: {
    padding: '2rem 1.5rem',
    display: 'flex',
    alignItems: 'center',
    gap: '1rem',
    borderBottom: '1px solid rgba(255, 255, 255, 0.1)',
  },
  logoIcon: {
    fontSize: '2rem',
  },
  logoText: {
    fontSize: '1.25rem',
    fontWeight: '700',
    color: 'white',
  },
  nav: {
    flex: 1,
    padding: '1.5rem 1rem',
    display: 'flex',
